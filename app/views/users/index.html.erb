<main>
  <div>
    <h2>Users</h2>
    <% @users.each do |user| %>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= image_tag user.get_profile_image(80,80) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
      </tbody>
    </table>
    <% end %>
  </div>
</main>